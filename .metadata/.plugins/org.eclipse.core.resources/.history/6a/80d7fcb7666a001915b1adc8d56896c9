package z_useful;
/*
 * 1. String
 * 2. StringBuffer
 * 3. StringBuilder = StringBuffer
 */
public class StringStrigBuffer {

	public static void main(String[] args) {
		
		String s = "안녕";
		s+="하세요";
		System.out.println(s);
		
		StringBuffer sb = new StringBuffer("반갑");
		sb.append("습니다.");
		System.out.println(sb);
	}

}
