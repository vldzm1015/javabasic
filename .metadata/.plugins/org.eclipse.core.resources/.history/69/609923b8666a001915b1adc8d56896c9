package c_control;

public class Ex01_if_주민번호{

	public static void main(String[] args) {
		String id = "880203-202545";
		char sung = id.charAt(7);
		// 1,3,9 남자를 출력
		// 2,4,8 여자를 출력
		if( sung =='1'    | sung =='3'   |sung =='9'   ) {
			System.out.println("남자");
		}else if( sung =='2'| sung =='4'   |sung =='8'    ) {
			System.out.println("여자");
		}			
	}
}
