package z_exercise;

import java.util.Scanner;

public class Ex01_주사위합 {

	public static void main(String[] args) {
		//입력값
		Scanner input = new Scanner(System.in);
				System.out.println("숫자를 입력하세요");
				char cha = input.next().charAt(0);
		
		//1.첫번째 주사위 수
		for(int i=1; i<6; i++) {
			//2.두번쨰 주사위 수
			for(int j=1; j<6; j++) {
				if( cha == i+j) {
					System.out.println( i+" "+j);
				}
			System.out.println( );
			}
		}
	}
}
