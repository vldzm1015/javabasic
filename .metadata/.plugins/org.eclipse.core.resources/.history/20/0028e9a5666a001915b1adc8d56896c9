package language;

import java.util.Scanner;

public class English extends Language {
	String greeting;
	String introduce;
	String say;
	
	English(){
	}
	
	public void greeting() {
		System.out.println(greeting+"Hello");
	}
	
	public void introduce() {
		System.out.println(introduce+"My name is Kildong Hong");
	}
	
	public void say() {
		System.out.println(say+"Nice to meet you");
	}
}


