package a_datatype;

public class Ex04_String {

   public static void main(String[] args) {
      String a ;
      a = new String("홍길동");
            
      String b = new String("홍길동");
      
      String c = "홍길북";
      
//      메모리 주소 비교
      if (a == b) {
         System.out.println("같다");
      }else {
         System.out.println("다르다");
      }

//      실제 값 비교
      if (a.equals(b)) {
         System.out.println("같다");
      }else {
         System.out.println("다르다");
      }
   }

}