package b_encapsulation;

public class Main {

	public static void main(String[] args) {

		Student s = new Student();
		s.setName("홍길동");
		s.setKor(100);
		s.setEng(88);
		s.setMath(77);
		System.out.println("이름 :"+s.getName());
		System.out.println("총점 : " +s.calTotal());
		//s.total=300;  
		System.out.println("평균 : " +s.calAverage());
	}
}
